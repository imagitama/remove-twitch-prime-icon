{
    "name": "Remove Twitch Prime Icon",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "A simple WebExtension that removes the Twitch Prime icon in all Twitch chats.",
    "homepage_url": "https://github.com/imagitama/remove-twitch-prime-icon",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "https://www.twitch.tv/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.twitch.tv/*"],
            "css": ["css/main.css"],
            "run_at": "document_end"
        }
    ]
}